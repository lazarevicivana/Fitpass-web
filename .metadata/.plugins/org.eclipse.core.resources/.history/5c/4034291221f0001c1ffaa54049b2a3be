var sportFacilities = new Vue({
    el:'#sportsFacilities',
    data:{
        sportFacilities: null,
        title: "Sports Facilities",
        mode: "BROWSE",
		user: {},
    },
    mounted(){
        axios.get('rest/facilities/GetAll')
             .then(response =>(this.sportFacilities = response.data))
    },
    methods:{
		showForm: function() {
			this.mode = "CREATE";
			//this.user = { id: null, name: null, surname: null, username: null, password: null, birthday: null, gender: null,userRole:null, };
		},
		convertStatus: function(sf) {
			
			if(sf.isWorking===true)
				return "Radi";
				else
				return "Ne radi";
		}
    }
});